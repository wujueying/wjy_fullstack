            <image src='/jd_images/a.png'style="height: 100%; width: 100%"></image>
          </swiper-item>
          <swiper-item>
            <image src='/jd_images/b.png'style="height: 100%; width: 100%"></image>
          </swiper-item>
          <swiper-item>
            <image src='/jd_images/c.png'style="height: 100%; width: 100%"></image>
          </swiper-item>
          
        </swiper>
      </view>
    </view>
  </scroll-view>
</view>
